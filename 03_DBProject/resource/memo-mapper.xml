<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="memo">

<select id = "selectAllMemo" resultType="m">
select * from memo_tbl order by 1
</select>

<select id ="selectOneMemo" resultType="m">
select * from memo_tbl where memo_no = #{memoNo}
</select>

<select id="searchWriter" resultType="m">
    select * from memo_tbl where memo_writer = #{memoWriter}
</select>

</mapper>
